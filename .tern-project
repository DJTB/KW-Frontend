{
  "ecmaVersion": 6,
  "libs": [],
  "loadEagerly": [
    "app/**/*.js"
  ],
  "dontLoad": [
    "app/*.js",
    "internals/**/*.js",
    "tests/**/*.js",
    "translations/**/*.js",
    "node_modules/**/*.js",
    "docs/**/*.js",
    "server/**/*.js"
  ],
  "plugins": {
    "modules": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "es_modules": {},
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    },
    "node": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    }
  }
}
