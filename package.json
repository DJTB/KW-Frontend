{
  "name": "kw-frontend",
  "version": "2.4.1",
  "description": "KaniWani frontend",
  "repository": {
    "type": "git",
    "url": "git://github.com/Kaniwani/kw-frontend.git"
  },
  "engines": {
    "npm": ">=5.5.1",
    "node": ">=8.9.1"
  },
  "author": "Duncan Bay",
  "license": "MIT",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "analyze": "node ./internals/scripts/analyze.js",
    "build:clean": "rimraf ./build",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js -p --progress --hide-modules",
    "cosmos": "cosmos",
    "cz": "git-cz",
    "format": "prettier-eslint --write --ignore '**/styles.js'",
    "lint": "eslint --color --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:app": "eslint --color --ignore-path .gitignore app/**/*.js",
    "postinstall": "npm run build:dll",
    "preanalyze": "npm run analyze:clean",
    "prebuild": "npm run build:clean",
    "sentry:upload": "sentry-cli releases files $npm_package_version upload-sourcemaps --rewrite ./build",
    "sentry:finalize": "sentry-cli releases finalize $npm_package_version",
    "start:prod": "cross-env NODE_ENV=production node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start": "cross-env NODE_ENV=development node server",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "test": "cross-env NODE_ENV=test jest"
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  },
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "sanitize.css"
    ],
    "include": [
      "core-js",
      "lodash",
      "eventsource-polyfill"
    ]
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/internals/mocks/cssModule.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/image.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/internals/testing/setupTests.js"
    ],
    "testRegex": "__tests__/.*\\.test\\.js$"
  },
  "dependencies": {
    "cuid": "2.1.4",
    "immutability-helper": "2.8.1",
    "localforage": "1.7.2",
    "lodash": "4.17.11",
    "polished": "2.2.0",
    "raphael": "2.2.7",
    "raven-for-redux": "1.3.1",
    "raven-js": "3.27.0",
    "react": "16.5.2",
    "react-autosize-textarea": "5.0.0",
    "react-collapse": "4.0.3",
    "react-dom": "16.5.2",
    "react-helmet": "5.2.0",
    "react-hotkeys": "1.1.4",
    "react-interval": "2.0.2",
    "react-loadable": "5.5.0",
    "react-measure": "2.1.2",
    "react-motion": "0.5.2",
    "react-network": "1.0.1",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-router-redux": "5.0.0-alpha.9",
    "react-tooltip": "3.8.4",
    "react-transition-group": "2.5.0",
    "rebass": "2.3.4",
    "recharts": "1.3.1",
    "recompose": "0.30.0",
    "redux": "4.0.0",
    "redux-actions": "2.6.1",
    "redux-form": "7.4.2",
    "redux-logic": "2.0.0",
    "redux-persist": "5.10.0",
    "reselect": "3.0.1",
    "reshader": "1.0.0",
    "sanitize.css": "7.0.3",
    "styled-components": "3.4.9",
    "voca": "1.4.0",
    "wanakana": "4.0.2",
    "wretch": "1.4.1"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "3.0.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "7",
    "babel-plugin-dynamic-import-node": "2.1.0",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-recharts": "1.1.1",
    "babel-plugin-styled-components": "1.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-plugin-transform-imports": "1.5.1",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.18",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chalk": "2.4.1",
    "circular-dependency-plugin": "5.0.2",
    "commitizen": "2.10.1",
    "compression": "1.7.3",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "cz-conventional-changelog": "2.1.0",
    "dotenv-webpack": "1.5.7",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "5.6.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.7.0",
    "express": "4.16.3",
    "fake-raf": "1.0.1",
    "faker": "4.1.0",
    "file-loader": "2.0.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.0.1",
    "image-webpack-loader": "4.3.1",
    "imports-loader": "0.8.0",
    "invariant": "2.2.4",
    "ip": "1.1.5",
    "jest-cli": "23.6.0",
    "jest-styled-components": "6.2.1",
    "lint-staged": "7.3.0",
    "minimist": "1.2.0",
    "ngrok": "3.1.0",
    "null-loader": "0.1.1",
    "offline-plugin": "5.0.5",
    "opn-cli": "3.1.0",
    "prettier-eslint-cli": "4.7.1",
    "prop-types": "15.6.2",
    "raf": "3.4.0",
    "redux-devtools-extension": "2.13.5",
    "rimraf": "2.6.2",
    "shelljs": "0.8.2",
    "style-loader": "0.23.0",
    "svg-react-loader": "0.4.5",
    "url-loader": "1.1.1",
    "url-search-params-polyfill": "5.0.0",
    "warning": "4.0.2",
    "webpack": "4.20.2",
    "webpack-bundle-analyzer": "3.0.2",
    "webpack-cli": "3.1.2",
    "webpack-dev-middleware": "3.4.0",
    "webpack-hot-middleware": "2.24.2",
    "webpack-pwa-manifest": "3.7.1",
    "whatwg-fetch": "3.0.0",
    "why-did-you-update": "0.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "post-merge": "yarn",
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  }
}
