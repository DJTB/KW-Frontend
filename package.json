{
  "name": "kw-frontend",
  "version": "2.6.2",
  "description": "KaniWani frontend",
  "repository": {
    "type": "git",
    "url": "git://github.com/Kaniwani/kw-frontend.git"
  },
  "engines": {
    "npm": ">=5.5.1",
    "node": ">=8.9.1"
  },
  "author": "Duncan Bay",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "analyze": "node ./internals/scripts/analyze.js",
    "build:clean": "rimraf ./build",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js -p --progress --hide-modules",
    "cosmos": "cosmos",
    "cz": "git-cz",
    "format": "prettier-eslint --write --ignore '**/styles.js'",
    "lint": "eslint --color --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:app": "eslint --color --ignore-path .gitignore app/**/*.js",
    "preanalyze": "npm run analyze:clean",
    "prebuild": "npm run build:clean",
    "release": "cross-env SENTRY_UPLOAD=true npm run build",
    "start:prod": "cross-env NODE_ENV=production node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start": "cross-env NODE_ENV=development node server",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "test": "cross-env NODE_ENV=test jest"
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "cuid": "2.1.6",
    "immutability-helper": "2.8.1",
    "localforage": "1.7.3",
    "lodash": "4.17.13",
    "polished": "2.3.3",
    "raphael": "2.2.8",
    "raven-for-redux": "1.4.0",
    "raven-js": "3.27.2",
    "react": "16.7.0",
    "react-autosize-textarea": "5.0.0",
    "react-collapse": "4.0.3",
    "react-dom": "16.7.0",
    "react-helmet": "5.2.1",
    "react-hotkeys": "1.1.4",
    "react-interval": "2.0.2",
    "react-loadable": "5.5.0",
    "react-measure": "2.2.2",
    "react-motion": "0.5.2",
    "react-network": "1.0.1",
    "react-redux": "5.1.0",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-router-redux": "5.0.0-alpha.9",
    "react-tooltip": "3.9.2",
    "react-transition-group": "2.5.3",
    "rebass": "2.3.4",
    "recharts": "1.4.2",
    "recompose": "0.30.0",
    "redux": "4.0.3",
    "redux-actions": "2.6.5",
    "redux-form": "7.4.2",
    "redux-logic": "2.1.1",
    "redux-persist": "5.10.0",
    "reselect": "4.0.0",
    "sanitize.css": "8.0.0",
    "styled-components": "4.1.3",
    "voca": "1.4.0",
    "wanakana": "4.0.2",
    "wretch": "1.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@sentry/webpack-plugin": "^1.6.1",
    "add-asset-html-webpack-plugin": "3.1.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.0.0",
    "babel-loader": "8.0.6",
    "babel-plugin-dynamic-import-node": "2.2.0",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-recharts": "1.2.0",
    "babel-plugin-styled-components": "1.10.6",
    "babel-plugin-transform-imports": "1.5.1",
    "babel-plugin-transform-react-remove-prop-types": "0.4.23",
    "chalk": "2.4.2",
    "circular-dependency-plugin": "5.0.2",
    "commitizen": "3.0.5",
    "compression": "1.7.4",
    "compression-webpack-plugin": "^2.0.0",
    "cross-env": "5.2.0",
    "css-loader": "2.1.0",
    "cz-conventional-changelog": "2.1.0",
    "dotenv-webpack": "1.7.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.8.0",
    "enzyme-to-json": "3.3.5",
    "eslint": "5.12.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-import-resolver-webpack": "0.11.0",
    "eslint-plugin-import": "2.15.0",
    "eslint-plugin-jsx-a11y": "6.2.0",
    "eslint-plugin-react": "7.12.4",
    "exports-loader": "0.7.0",
    "express": "4.16.4",
    "fake-raf": "1.0.1",
    "faker": "4.1.0",
    "file-loader": "3.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.3.1",
    "image-webpack-loader": "4.6.0",
    "imports-loader": "0.8.0",
    "invariant": "2.2.4",
    "ip": "1.1.5",
    "jest-cli": "24.0.0",
    "jest-styled-components": "^7.0.0-2",
    "lint-staged": "7.3.0",
    "minimist": "1.2.0",
    "ngrok": "3.1.0",
    "offline-plugin": "5.0.7",
    "prettier-eslint-cli": "4.7.1",
    "prop-types": "15.6.2",
    "raf": "3.4.1",
    "react-app-polyfill": "^0.2.0",
    "redux-devtools-extension": "2.13.8",
    "rimraf": "2.6.3",
    "shelljs": "0.8.3",
    "style-loader": "0.23.1",
    "svg-react-loader": "0.4.6",
    "uglifyjs-webpack-plugin": "2.1.3",
    "url-loader": "1.1.2",
    "url-search-params-polyfill": "5.0.0",
    "warning": "4.0.3",
    "webpack": "4.29.0",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.2.1",
    "webpack-dev-middleware": "3.5.1",
    "webpack-hot-middleware": "2.24.3",
    "webpack-pwa-manifest": "3.7.1",
    "why-did-you-update": "0.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "post-merge": "yarn",
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  }
}
