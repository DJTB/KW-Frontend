{
  "name": "kw-frontend",
  "version": "2.0.0",
  "description": "KaniWani frontend",
  "repository": {
    "type": "git",
    "url": "git://github.com/Kaniwani/kw-frontend.git"
  },
  "engines": {
    "npm": ">=5.5.1",
    "node": ">=8.9.1"
  },
  "browserslist": [
    "last 4 versions",
    ">1%"
  ],
  "author": "Duncan Bay",
  "license": "MIT",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "analyze": "node ./internals/scripts/analyze.js",
    "build:clean": "rimraf ./build",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js -p --progress --hide-modules",
    "cosmos": "cosmos",
    "coverage:view": "opn coverage/lcov-report/index.html",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "cz": "git-cz",
    "format": "prettier-eslint --write --ignore '**/styles.js'",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "eslint --color --ignore-path .gitignore --ignore-pattern internals/scripts",
    "postinstall": "npm run build:dll",
    "preanalyze": "npm run analyze:clean",
    "prebuild": "npm run build:clean",
    "precommit": "lint-staged",
    "postmerge": "yarn",
    "pretest": "npm run test:clean",
    "start:prod": "cross-env NODE_ENV=production node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start": "cross-env NODE_ENV=development node server",
    "test:clean": "rimraf ./coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "test": "cross-env NODE_ENV=test jest --coverage"
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  },
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "sanitize.css"
    ],
    "include": [
      "core-js",
      "lodash",
      "eventsource-polyfill"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "app/**/*.{js}",
      "!app/**/*.test.{js}",
      "!app/**/*.fixture.js",
      "!app/**/styles.js",
      "!app/common/assets/*.*",
      "!app/common/data/*.*",
      "!app/common/styles/**.js",
      "!app/index.js",
      "!app/*/*/Loadable.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 30,
        "branches": 30,
        "functions": 30,
        "lines": 30
      }
    },
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/internals/mocks/cssModule.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/image.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>/internals/testing/setupTests.js"
    ],
    "testRegex": "__tests__/.*\\.test\\.js$"
  },
  "dependencies": {
    "cuid": "2.0.2",
    "deep-map-keys": "1.2.0",
    "immutability-helper": "2.6.4",
    "localforage": "1.5.6",
    "lodash": "4.17.4",
    "polished": "1.9.0",
    "raphael": "2.2.7",
    "react": "16.2.0",
    "react-autosize-textarea": "3.0.1",
    "react-collapse": "4.0.3",
    "react-dom": "16.2.0",
    "react-helmet": "5.2.0",
    "react-hotkeys": "0.10.0",
    "react-interval": "2.0.2",
    "react-loadable": "5.3.1",
    "react-measure": "2.0.2",
    "react-motion": "0.5.2",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "react-tooltip": "3.4.0",
    "rebass": "1.0.4",
    "recharts": "1.0.0-beta.7",
    "recompose": "0.26.0",
    "redux": "3.7.2",
    "redux-action-types-creator": "1.2.0",
    "redux-actions": "2.2.1",
    "redux-form": "7.2.1",
    "redux-logic": "0.12.3",
    "redux-persist": "5.5.0",
    "reselect": "3.0.1",
    "sanitize.css": "5.0.0",
    "styled-components": "2.4.0",
    "voca": "1.4.0",
    "wanakana": "3.0.0",
    "whatwg-fetch": "2.0.3",
    "wretch": "1.1.1"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "2.1.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-loader": "7.1.2",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-styled-components": "1.4.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-imports": "1.4.1",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.12",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chalk": "2.3.0",
    "circular-dependency-plugin": "4.4.0",
    "commitizen": "2.9.6",
    "compression": "1.7.1",
    "coveralls": "3.0.0",
    "cross-env": "5.1.3",
    "css-loader": "0.28.9",
    "cz-conventional-changelog": "2.1.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-to-json": "3.3.0",
    "eslint": "4.16.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-import-resolver-webpack": "0.8.4",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.5.1",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.6.4",
    "express": "4.16.2",
    "fake-raf": "1.0.1",
    "faker": "4.1.0",
    "file-loader": "1.1.6",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.14.3",
    "image-webpack-loader": "3.4.2",
    "imports-loader": "0.7.1",
    "invariant": "2.2.2",
    "ip": "1.1.5",
    "jest-cli": "22.1.4",
    "jest-styled-components": "4.10.0",
    "lint-staged": "6.0.1",
    "minimist": "1.2.0",
    "ngrok": "2.2.25",
    "node-plop": "0.9.0",
    "null-loader": "0.1.1",
    "offline-plugin": "4.9.0",
    "opn-cli": "3.1.0",
    "plop": "1.9.1",
    "prettier-eslint-cli": "4.7.0",
    "prop-types": "15.6.0",
    "raf": "3.4.0",
    "react-cosmos": "3.2.1",
    "react-cosmos-fetch-proxy": "3.1.1",
    "react-cosmos-localstorage-proxy": "3.1.1",
    "react-cosmos-redux-proxy": "3.1.1",
    "react-cosmos-router-proxy": "3.1.1",
    "react-cosmos-wrapper-proxy": "1.1.1",
    "react-x-ray": "1.0.0-4",
    "redux-devtools-extension": "2.13.2",
    "redux-freeze": "0.1.5",
    "redux-logic-test": "1.0.5",
    "rimraf": "2.6.2",
    "shelljs": "0.8.0",
    "style-loader": "0.19.1",
    "svg-react-loader": "0.4.5",
    "url-loader": "0.6.2",
    "warning": "3.0.0",
    "webpack": "3.10.0",
    "webpack-dev-middleware": "2.0.4",
    "webpack-hot-middleware": "2.21.0",
    "why-did-you-update": "0.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "//": "This rule allows js-hyperclick (atom editor plugin) to resolve absolute imported modules properly",
  "moduleRoots": [
    "app"
  ],
  "quokka": {
    "babel": true
  }
}
